# mount     fstype  device                  [device2]   [fstype2]   [fsoptions]       [fsoptions2]

/cache      ext4    /dev/block/stl10       NULL        rfs         NULL              rw,nosuid,nodev,check=no
/data       ext4    /dev/block/stl11       NULL        rfs         NULL              rw,nosuid,nodev,check=no
/system     ext4    /dev/block/stl9        NULL        rfs         NULL              rw,check=no
/sdcard     vfat    /dev/block/mmcblk0p1
/sd-ext     ext4    /dev/block/mmcblk0p2   NULL        auto        NULL              rw,nosuid,nodev,check=no

